{
  "version": 2,
  "builds": [
      {
          "src": "/src/server.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/"
      }
  ],
  "headers": [
      {
          "source": "/(.*)",
          "headers": [
              { "key": "Access-Control-Allow-Origin", "value": "*" },
              { "key": "Access-Control-Allow-Methods", "value": "GET,POST,PUT,DELETE,PATCH,HEAD,OPTIONS" }
          ]
      }
  ]
}